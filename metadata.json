{
  "name": "larkit-host_railsapp",
  "version": "0.1.0",
  "summary": "Hosting setup for Ruby on Rails Apps",
  "author": "Tommy McNeely",
  "description": "Setup a hosting environment for Ruby on Rails apps using RVM for Ruby and Phusion Passenger / Nginx as the web container.",
  "source": "http://github.com/LarkIT/puppet-host_railsapp",
  "project_page": "http://github.com/LarkIT/puppet-host_railsapp",
  "license": "Apache-2.0",
  "issues_url": "https://github.com/LarkIT/larkit-phppet-host_railsapp/issues",
  "operatingsystem_support": [
    { "operatingsystem": "RedHat", "operatingsystemrelease": [ "6", "7" ] }
  ],
  "dependencies": [
    { "name": "maestrodev/rvm", "version_requirement": ">= 1.4.1 <2.0.0" },
    { "name": "puppetlabs-stdlib", "version_requirement": ">= 4.6.0" }
  ]
}
